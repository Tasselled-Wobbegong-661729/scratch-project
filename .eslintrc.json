{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["warn", 2],
        "no-unused-vars": ["off", { "vars": "local" }],
        "prefer-const": "warn",
        "quotes": ["warn", "single"],
        "react/prop-types": "off",
        "semi": ["warn", "always"],
        "space-infix-ops": "warn"
    }
}
